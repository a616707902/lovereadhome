<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DotPageIndicator">
        <attr name="dotColorSelected" format="color"/>
        <attr name="dotColor" format="color"/>
        <attr name="dotRadius" format="dimension"/>
        <attr name="dotSpacing" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SettingItemView">
        <attr name="leftImg" format="reference" />
        <attr name="titleText" format="string" />
        <attr name="rightImg" format="reference" />
        <attr name="contentColor" format="color" />
        <attr name="titleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="x_image_view">
        <attr name="img_src" format="reference|color"></attr>
        <attr name="default_src" format="reference|dimension"></attr>
        <attr name="showPercent" format="boolean"></attr>
    </declare-styleable>
</resources>